      <!-- Converter for seller Import -->
      <bean id="batchLuxurySellerPriorityConverter" class="de.hybris.platform.acceleratorservices.dataimport.batch.converter.impl.DefaultImpexConverter">
        <property name="header">
            <value>#{defaultImpexProductHeader}
              	$catalogVersion=catalogversion(catalog(id[default=luxProductCatalog]),version[default='Online'])[unique=true]
				$approved=approvalstatus(code)[default='check']
				$lang=en
                $productCatalog=luxProductCatalog
				$catalogVersion=catalogversion(catalog(id[default=$productCatalog]),version[default='Online'])[unique=true,default=$productCatalog:Online]
				$supercategories=supercategories(code, $catalogVersion)
				$baseProduct=baseProduct(code,$catalogVersion)
				$approved=approvalstatus(code)[default='check']
				$unit=unit(code)[default='pieces']
				$clAttrModifiers=system='pcmClassification',version='1',translator=de.hybris.platform.catalog.jalo.classification.impex.ClassificationAttributeTranslator,lang=en
				$catalogversion=catalogversion(catalog(id[default=luxProductCatalog]),version[default='Online'])[unique=true,default=luxProductCatalog:Online]
				Insert_Update MplSellerPriority;ListingId[unique=true][ignoreNull=true](code,$catalogversion);CategoryId[unique=true][ignoreNull=true](code,$catalogversion);SellerId(id);isActive;priorityStartDate[dateformat=dd/MM/yyyy HH:mm:ss];priorityEndDate[dateformat=dd/MM/yyyy HH:mm:ss]
			</value>
        </property>
		<property name="impexRow">
		<value>;{0};{1};{+2};{3};{4};{5}</value>
		</property>
        </bean>
    
