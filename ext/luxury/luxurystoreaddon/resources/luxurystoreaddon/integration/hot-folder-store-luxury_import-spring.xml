 		<!-- General setup -->
 		
 		 <bean id="luxFileOrderComparator" class="de.hybris.platform.acceleratorservices.dataimport.batch.FileOrderComparator">
		<property name="prefixPriority">
			<map>
				<!-- default priority is 0 -->
				<entry key="category" value="15" />
				<entry key="mplproduct" value="14" />
				<entry key="variant_product" value="13" />
				<entry key="seller" value="12" />
				<entry key="richattribute_seller" value="11" />
				<entry key="richattribute_product" value="10" />
				<entry key="brand" value="9" />
				<entry key="seocontent" value="8" />
				<entry key="global_identifier" value="7" />
				<entry key="product_reference" value="6" />
				<entry key="product_feature" value="5" />
				<entry key="mplmedia" value="4" />
				<entry key="mplmedia_container" value="3" />
				<entry key="mplmedia_product" value="2" />
				<entry key="category_media" value="1" /> 
				<entry key="product_sizeguide" value="0" />
				<entry key="online_exclusive" value="0" />
				<entry key="priority_seller" value="0"/>
				<entry key="promoted_product" value="0"/>
			</map>
		</property>
	</bean>
 
    <bean id="baseDirectoryLuxuryCategory" class="java.lang.String">
        <constructor-arg value="#{baseDirectory}/${tenantId}/luxury-import" />
    </bean>
 
    <!-- 1) Scan for files -->
    
    <file:inbound-channel-adapter id="batchFilesLuxuryCategory" directory="#{baseDirectoryLuxuryCategory}"
        filename-regex="^(.*)-(\d+)\.csv" comparator="luxFileOrderComparator" auto-startup="#{configurationService.configuration.getProperty('etail.batch.import.hotfolder.enabled')}">
        <int:poller fixed-rate="1000" />
    </file:inbound-channel-adapter>
 
    <!-- 2) move the file to processing and setup header -->
    
    <file:outbound-gateway request-channel="batchFilesLuxuryCategory" reply-channel="batchFilesLuxuryCategoryProc"
        directory="#{baseDirectoryLuxuryCategory}/processing" delete-source-files="true" filename-generator-expression="'lux_' + payload.name"/>

	<!-- 3) transform csv to impex file -->
	
 	<int:service-activator input-channel="batchFilesLuxuryCategoryProc" output-channel="batchFilesHeaderInit"
        ref="LuxuryCategoryHeaderSetupTask" method="execute" />
 
 	<bean id="LuxuryCategoryHeaderSetupTask" class="de.hybris.platform.acceleratorservices.dataimport.batch.task.HeaderSetupTask">
        <property name="catalog" value="luxProductCatalog" />
        <property name="net" value="false" />
        <property name="storeBaseDirectory" ref="baseDirectoryLuxuryCategory" />
    </bean>   
    
