	<!-- Consignment Process Actions -->

	<bean id="sendConsignmentToWarehouseAction" class="com.tisl.mpl.fulfilmentprocess.actions.consignment.SendConsignmentToWarehouseAction"  parent="abstractAction">
		<property name="process2WarehouseAdapter" ref="process2WarehouseAdapter"/>
	</bean>

	<bean id="waitBeforeTransmissionAction" class="com.tisl.mpl.fulfilmentprocess.actions.consignment.WaitBeforeTransmissionAction"  parent="abstractAction"/>

