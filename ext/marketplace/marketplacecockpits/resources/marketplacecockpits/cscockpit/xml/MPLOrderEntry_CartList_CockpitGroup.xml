<?xml version="1.0" encoding="UTF-8"?>
<list-view>
	<group name="General">
		<group name="column">
			<label key="config.general.transactionid" />
			<group name="line">
				<property qualifier="AbstractOrderEntry.transactionID"
					visible="true" />
			</group>
		</group>
		<group name="column">
			<label key="config.general.product" />
			<group name="line">
				<property qualifier="AbstractOrderEntry.product.code"
					visible="true" />
				<!-- <property qualifier="AbstractOrderEntry.product.manufacturerName"
					visible="true" /> -->
				<custom
					class="de.hybris.platform.cscockpit.services.config.impl.ProductNameColumn"
					name="Product" visible="true">
					<!-- <label key="cscockpit.OrderEntry.ean" /> -->
				</custom>
			</group>
			<group name="line">
				<!-- <group name="Unit">
					<label key="config.general.units" />
					<property qualifier="AbstractOrderEntry.product.unit"
						visible="true" />
				</group> -->
				<group name="Ean">
					<label key="cscockpit.OrderEntry.selectedUSSID" />
					<property qualifier="AbstractOrderEntry.selectedUSSID"
						visible="true" />
				</group>
			</group>
		</group>
		<group name="column">
			<label key="config.general.orderlinestatus" />
			<custom
				class="com.tisl.mpl.cockpits.services.config.impl.OrderStatusColumn"
				name="OrderLine Status" visible="true">
			</custom>
		</group>
		<group name="column">
			<label key="config.general.deliverymode" />
				<custom class="com.tisl.mpl.cockpits.services.config.impl.DeliveryModeColumn"
					visible="true" />
		</group>
	</group>
</list-view>
