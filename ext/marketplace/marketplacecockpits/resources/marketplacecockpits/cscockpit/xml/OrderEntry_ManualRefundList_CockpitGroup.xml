<?xml version="1.0" encoding="UTF-8"?>
<list-view>
	<group name="General">
		<group name="column">
			<label key="config.general.transactionid" />

			<property qualifier="RefundEntry.orderEntry.transactionID"
				visible="true" />

		</group>
		<group name="column">
			<label key="config.general.product.invoice" />

			<property qualifier="RefundEntry.orderEntry.product.name"
				visible="true" />

		</group>
		<group name="column">
			<label key="config.general.refundentry.sellerName" />
			<property qualifier="RefundEntry.orderEntry.sellerInfo"
				visible="true" />

		</group>
		<group name="column">
			<label key="config.general.refundReason" />

			<property qualifier="RefundEntry.reason" visible="true" />

		</group>
		<group name="column">
			<label key="config.general.refundentry.status" />

			<custom
				class="com.tisl.mpl.cockpits.services.config.impl.OrderStatusColumn"
				visible="true">
			</custom>

		</group>
		<group name="column">
			<label key="config.general.refundAmount" />

			<property qualifier="RefundEntry.orderEntry.netAmountAfterAllDisc" visible="true" />

		</group>
	</group>
</list-view>