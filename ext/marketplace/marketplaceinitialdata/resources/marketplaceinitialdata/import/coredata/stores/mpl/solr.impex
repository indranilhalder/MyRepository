# Create the queries that will be used to extract data for Solr
INSERT_UPDATE SolrIndexerQuery;solrIndexedType(identifier)[unique=true];identifier[unique=true];type(code);injectCurrentDate[default=true];injectCurrentTime[default=true];injectLastIndexTime[default=true];query;user(uid)
;$solrIndexedType;$searchIndexNamePrefix-fullQuery;full;;;false;"
 SELECT  DISTINCT {P.PK}  FROM {BUYBOX AS BBOX  JOIN PRODUCT AS P ON {P.CODE}={BBOX.PRODUCT}  JOIN CATEGORYPRODUCTRELATION AS C2P ON {P.PK}={C2P.TARGET}  JOIN CATEGORY AS C ON {C2P.SOURCE}={C.PK} }  WHERE ({C.CODE} LIKE 'MSH%' OR {C.CODE} LIKE 'LSH%') AND  {BBOX.DELISTED} != 1  AND {BBOX.PRICE} > $freebiePriceThreshold  AND ( SYSDATE BETWEEN {BBOX.SELLERSTARTDATE} AND {BBOX.SELLERENDDATE} ) 
 ";anonymous
;$solrIndexedType;$searchIndexNamePrefix-updateQuery;update;;;;"
SELECT distinct pk,code from ({{SELECT {p.PK} as pk , {p:code} AS code FROM {ProductPromotion AS PP JOIN ProductPromotionRelation AS PPR ON {PPR.target} = {PP.PK} JOIN Product AS p ON {p.PK} = {PPR.SOURCE} JOIN BuyBox AS bbox ON {p.code}={bbox.product} JOIN CategoryProductRelation AS c2p ON {p.PK}={c2p.target} JOIN Category AS c ON {c2p.source}={c.PK} } WHERE ({c.code} LIKE 'MSH%' OR {c.code} LIKE 'LSH%') AND {PPR:modifiedtime} >= (?lastIndexTime - 15/1440) AND {bbox.delisted} != 1 AND {bbox.price} > $freebiePriceThreshold AND ( sysdate BETWEEN {bbox.sellerstartdate} AND {bbox.sellerenddate} ) }} UNION {{SELECT  {p.PK} as pk , {p:code} AS code FROM {BuyBox AS bbox JOIN Product AS p ON {p.code}={bbox.product} JOIN CategoryProductRelation AS c2p ON {p.PK}={c2p.target} JOIN Category AS c ON {c2p.source}={c.PK}} WHERE ({c.code} LIKE 'MSH%' OR {c.code} LIKE 'LSH%') AND ({bbox:modifiedtime} >= (?lastIndexTime - 15/1440) OR  {p:modifiedtime} >= (?lastIndexTime - 15/1440)) AND {bbox.delisted} != 1 AND {bbox.price} > $freebiePriceThreshold AND ( sysdate BETWEEN {bbox.sellerstartdate} AND {bbox.sellerenddate} )}})
";anonymous
;$solrIndexedType;$searchIndexNamePrefix-deleteQuery;delete;;;;"SELECT distinct{p.PK} FROM {Product AS p} 						WHERE {p.code} IN ( 										{{ SELECT distinct{bbox.product} FROM 	{BuyBox AS bbox} 													WHERE 	{bbox:modifiedtime} + 15/1440>= ?lastIndexTime 													AND ({bbox.product}={p.code} 													AND ({bbox.delisted} = 1 														OR (sysdate not between {bbox.sellerstartdate} and {bbox.sellerenddate})))													AND {bbox.product} NOT IN (													{{														SELECT distinct{bbox.product} FROM {BuyBox AS bbox} 															WHERE {bbox.delisted} != 1 															AND (sysdate between {bbox.sellerstartdate} and {bbox.sellerenddate})													}})										}} 									) 						AND {p.varianttype} IS NULL
";anonymous

