<items 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xsi:noNamespaceSchemaLocation="items.xsd">

<relations>
	<relation code="BinBankRelation" generate="true"
			localized="false" autocreate="true">
			<sourceElement qualifier="bin" type="Bin"
				cardinality="many" />
			<targetElement qualifier="bank" type="Bank"
				cardinality="one" />
	</relation> 
</relations>

<itemtypes>

	<!-- BIN Model for BIN Validation -->
	<itemtype code="Bin" autocreate="true" generate="true">
		<description>BIN numbers for BIN Validation of Debit/Credit Cards</description>
		<deployment table="MplBinDataTable" typecode="15236" />
		<attributes>
			<attribute qualifier="BinNo" type="java.lang.String">
				<description>Defines the BIN no.</description>
				<persistence type="property" />
				<modifiers  optional="false" />
			</attribute>
			<attribute qualifier="bankName" type="java.lang.String">
				<description>Defines the Bank Name.</description>
				<persistence type="property"  />
			</attribute>
			<attribute qualifier="CardBrand" type="java.lang.String">
				<description>Brand of the card against the BIN.</description>
				<persistence type="property" />
			</attribute>
			<attribute qualifier="CardType" type="java.lang.String">
				<description>Type of card against the BIN.</description>
				<persistence type="property" />
			</attribute>
			<attribute qualifier="Category" type="java.lang.String">
				<description>Category of card against the BIN.</description>
				<persistence type="property" />
			</attribute>
			<attribute qualifier="IssuingCountry" type="java.lang.String">
				<description>Issuing Country of card against the BIN.</description>
				<persistence type="property" />
			</attribute>
			<attribute qualifier="ISOA2" type="java.lang.String">
				<description> issuing country ISO A2 code of card against the BIN.</description>
				<persistence type="property" />
			</attribute>
			<attribute qualifier="ISOA3" type="java.lang.String">
				<description> issuing country ISO A3 code of card against the BIN.</description>
				<persistence type="property" />
			</attribute>
			<attribute qualifier="ISONumber" type="java.lang.String">
				<description> issuing country ISO Number code of card against the BIN.</description>
				<persistence type="property" />
			</attribute>
			<attribute qualifier="IssuingOrgWebsite" type="java.lang.String">
				<description>Issuing Organization Website of card against the BIN.</description>
				<persistence type="property" />
			</attribute>
			<attribute qualifier="IssuingOrgPhoneNo" type="java.lang.String">
				<description>Issuing Organization phone no. of card against the BIN.</description>
				<persistence type="property" />
			</attribute>
			<attribute qualifier="version" type="java.lang.String">
				<description>Defines the Version of the Data</description>
				<persistence type="property"  />
			</attribute>
		</attributes>
	</itemtype>
	
</itemtypes>
</items>
