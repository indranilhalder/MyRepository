	 <widget-extension widgetId="tataBUCperspective">
	<move widgetId="backofficeMainSlot" targetWidgetId="backofficeMainSlot" targetSlotId="perspectives" position="0"/>
	</widget-extension>
	
	<widget-connection inputId="input" outputId="nodeSelected"
		sourceWidgetId="tataomsbackoffice-explorer-tree" targetWidgetId="tataomsbackoffice-tree-node-adapter" />
	<widget-connection inputId="genericInput" outputId="true"
		sourceWidgetId="tataomsbackoffice-tree-node-adapter" targetWidgetId="tataomsbackoffice-typecode-extractor" />
	<widget-connection inputId="genericInput" outputId="true"
		sourceWidgetId="tataomsbackoffice-tree-node-adapter" targetWidgetId="tataomsbackoffice-search-enabler" />
	<widget-connection inputId="genericInput" outputId="false"
		sourceWidgetId="tataomsbackoffice-tree-node-adapter" targetWidgetId="tataomsbackoffice-search-disabler" />
	<widget-connection inputId="type" outputId="genericOutput"
		sourceWidgetId="tataomsbackoffice-typecode-extractor" targetWidgetId="tataomsbackoffice-search-logic" />
	<widget-connection inputId="enabled" outputId="genericOutput"
		sourceWidgetId="tataomsbackoffice-search-enabler" targetWidgetId="tataomsbackoffice-text-search" />
	<widget-connection inputId="enabled" outputId="genericOutput"
		sourceWidgetId="tataomsbackoffice-search-disabler" targetWidgetId="tataomsbackoffice-text-search" />
	<widget-connection inputId="searchtext" outputId="query"
		sourceWidgetId="tataomsbackoffice-text-search" targetWidgetId="tataomsbackoffice-search-logic" />
	<widget-connection inputId="pageable" outputId="pageable"
		sourceWidgetId="tataomsbackoffice-search-logic" targetWidgetId="tataomsbackoffice-simple-list" />

	<!-- Socket connections for exploreretree -->
	<widget-connection sourceWidgetId="tataomsexplorerTree"
		outputId="nodeSelected" targetWidgetId="tataomsPerspectiveContainer"
		inputId="selectPerspective" />

	<!--Socket connections for deliverymodeconfig -->
	<widget-connection sourceWidgetId="deliverymodeconfig-listview"
		outputId="editorObject" inputId="editorObject" targetWidgetId="deliverymodeconfig-editor" />
	<widget-connection sourceWidgetId="deliverymodeconfig-editor"
		outputId="editorStatus" inputId="editorStatus" targetWidgetId="deliverymodeconfig-listview" />

	<!-- Socket connections for Logistics calendar -->

	<widget-connection sourceWidgetId="logistic-calendar-listview"
		outputId="createlogisticscalendar" inputId="isactivecreate"
		targetWidgetId="logistic-calendar-editor-perspective" />

	<widget-connection sourceWidgetId="logistic-calendar-listview"
		outputId="editlogisticscalendar" inputId="isactiveedit"
		targetWidgetId="logistic-calendar-editor-perspective" />

	<widget-connection sourceWidgetId="logistic-calendar-listview"
		outputId="deletelogisticscalendar" inputId="calendar_disble_contact_editor"
		targetWidgetId="logistic-calendar-editor-perspective" />

	<widget-connection sourceWidgetId="logistic-calendar-editor-perspective"
		outputId="refreshlogisticslist" inputId="listitemlogisticscalendar"
		targetWidgetId="logistic-calendar-listview" />

	<widget-connection sourceWidgetId="logistic-calendar-editor-perspective"
		outputId="refreshlogisticslist_edit" inputId="listitemlogisticscalendar_edit"
		targetWidgetId="logistic-calendar-listview" />



	<!-- Socket connections for Logistics contacts -->
	<widget-connection sourceWidgetId="logistic-contacts-listview"
		outputId="createlogisticscontact" inputId="isactivecreate_contact"
		targetWidgetId="logistic-contacts-editor-perspective" />

	<widget-connection sourceWidgetId="logistic-contacts-listview"
		outputId="editlogisticscontact" inputId="isactiveedit_contact"
		targetWidgetId="logistic-contacts-editor-perspective" />

	<widget-connection sourceWidgetId="logistic-contacts-editor-perspective"
		outputId="refreshlogisticscontactlist" inputId="listitemlogisticscontact"
		targetWidgetId="logistic-contacts-listview" />

	<widget-connection sourceWidgetId="logistic-contacts-editor-perspective"
		outputId="refreshlogisticscontactlist_edit" inputId="listitemlogisticscontact_edit"
		targetWidgetId="logistic-contacts-listview" />

	<widget-connection sourceWidgetId="logistic-contacts-listview"
		outputId="deletelogisticscalendar" inputId="contact_disble_contact_editor"
		targetWidgetId="logistic-contacts-editor-perspective" />


	<!-- SocketConnection For InventorySearchByUSSID -->
	<widget-connection sourceWidgetId="inventory-by-ussid-searchwidget"
		outputId="searchedText" inputId="ussid" targetWidgetId="inventory-by-ussid-listview" />
	<!-- socketConnections for Pincode Logistics Reports -->	
	<widget-connection sourceWidgetId="pincode_logistics_report_datepic" outputId="startendDates" targetWidgetId="pincode_logistics_report_listview"  inputId="startendDates" />
		
    <widget-connection sourceWidgetId="tataBUCperspective" outputId="possiblePerspectives" targetWidgetId="backofficePerspectiveChooser" inputId="possiblePerspectives"/>
    <widget-connection sourceWidgetId="backofficePerspectiveChooser" outputId="selectPerspective" targetWidgetId="tataBUCperspective" inputId="selectPerspective"/>

