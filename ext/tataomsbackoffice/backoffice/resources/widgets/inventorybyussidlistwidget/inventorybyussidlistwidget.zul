		<listhead>
			<listheader label="${labels.inv.slave}"></listheader>
			<listheader label="${labels.inv.ats}"></listheader>
			<listheader label="${labels.inv.onhand}"></listheader>
			<listheader label="${labels.inv.reservd}"></listheader>
			<listheader label="${labels.inv.allctd}"></listheader>
		</listhead>
	</listbox>
	
	<hlayout   sclass="label_hl" id="v_total">
	 <label sclass="label_css" id="pendingAssignment" value=""/>  
	 <label sclass="label_css" id="totalAts" value=""/> 
	</hlayout>

</widget>
